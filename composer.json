{
    "name": "vjik/yii-multiapp",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    },
    "require": {
        "php": "^7.4",
        "nyholm/psr7": "^1.3",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "yiisoft/data-response": "@dev",
        "yiisoft/di": "@dev",
        "yiisoft/log": "@dev",
        "yiisoft/log-target-file": "@dev",
        "yiisoft/router-fastroute": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/yii-console": "@dev",
        "yiisoft/yii-cycle": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Common\\": "common/src",
            "AppMain\\": "app-main/src"
        }
    },
    "extra": {
        "config-plugin-output-dir": "runtime/config",
        "config-plugin": {
            "params": [
                "common/config/params.php",
                "?common/config/params-local.php"
            ],
            "common": "common/config/common.php",
            "web": [
                "$common",
                "common/config/web.php"
            ],
            "console": [
                "$common",
                "common/config/console.php"
            ],
            "app-main": [
                "$web",
                "app-main/config/main.php"
            ],
            "events": "common/config/events.php",
            "events-web": [
                "$events",
                "common/config/events-web.php"
            ],
            "events-console": [
                "$events",
                "common/config/events-console.php"
            ],
            "events-app-main": [
                "$events-web",
                "app-main/config/events.php"
            ],
            "providers": "common/config/providers.php",
            "providers-web": [
                "$providers",
                "common/config/providers-web.php"
            ],
            "providers-console": [
                "$providers",
                "common/config/providers-console.php"
            ],
            "providers-app-main": [
                "$providers-web",
                "app-main/config/providers.php"
            ],
            "routes-app-main": "app-main/config/routes.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
