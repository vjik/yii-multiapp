{
    "name": "vjik/yii-multiapp",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    },
    "require": {
        "php": "^7.4",
        "nyholm/psr7": "^1.3",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "yiisoft/data-response": "@dev",
        "yiisoft/di": "@dev",
        "yiisoft/log": "@dev",
        "yiisoft/log-target-file": "@dev",
        "yiisoft/router-fastroute": "@dev",
        "yiisoft/yii-console": "@dev",
        "yiisoft/yii-web": "@dev"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Common\\": "common",
            "AppMain\\": "app-main"
        }
    },
    "extra": {
        "config-plugin-output-dir": "runtime/config",
        "config-plugin": {
            "params": "config/common/params.php",
            "common": "config/common/common.php",
            "web": "config/common/web.php",
            "console": [
                "$common",
                "config/console/main.php"
            ],
            "app-main": [
                "$common",
                "$web",
                "config/app-main/main.php"
            ],
            "providers": "config/common/providers.php",
            "providers-web": [
                "$providers",
                "config/common/providers-web.php"
            ],
            "providers-console": [
                "$providers",
                "config/console/providers.php"
            ],
            "providers-app-main": [
                "$providers-web",
                "config/app-main/providers.php"
            ],
            "events": "config/common/events.php",
            "events-web": [
                "$events",
                "config/common/events-web.php"
            ],
            "events-console": [
                "$events",
                "config/console/events.php"
            ],
            "events-app-main": [
                "$events-web",
                "config/app-main/events.php"
            ],
            "routes-app-main": "config/app-main/routes.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
